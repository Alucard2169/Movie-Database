generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model movies {
  id      String @id @default(auto()) @map("_id") @db.ObjectId
  v       Int    @map("__v")
  movieId String
  type    String
  userID  String
  user   users   @relation(fields: [userID], references: [id])
}

model users {
  id       String @id @default(auto()) @map("_id") @db.ObjectId
  v        Int    @map("__v")
  password String
  username String
  movie movies[]
}
