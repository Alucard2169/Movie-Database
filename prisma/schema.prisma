generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model movies {
  id       String    @id @default(auto()) @map("_id") @db.ObjectId
  movieId    String
  type     String
  userId String    @db.ObjectId
  user   users      @relation(fields: [userId], references: [id])
}


model users {
  id      String   @id @default(auto()) @map("_id") @db.ObjectId

  username    String @unique
  password   String  
  movie movies[]
}